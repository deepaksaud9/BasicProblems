package LinkedList;

public class LL {
	Node head;
	private int size;
	
	class Node{
		String data;
		Node next;
		Node(String data){
			this.data = data;
			this.next = null;
		}
	}
	
	
	//insertion atFirst and atLast
	public void addFirst(String data) {
		Node newNode = new Node(data);
		
		if(head == null) {
			head = newNode;
			return;
		}
		newNode.next = head;
		head = newNode; 
	}
		
	//insertion in atLast Position 
		public void addLast(String data) {
			Node newNode = new Node(data);
			if(head == null) {
				head = newNode;
				return;
			}
			
			 while(head.next != null){
				 
			 }
		}
	
	
	public static void main(String[] args) {
		LL list = new LL();
		list.addFirst("a");
		list.addFirst("is");
	}

}
