package CoreJavaBasicProblem;

public class factorial {
	
	public static void main(String[] args) {
		
	int	count = 4;
	
	if(count<=0)
	{
		System.out.println("factorial of given number is 0");
		
		
	}else {
		
		for(int i=1;i<=3;i++)
		{
			count = count*i;
		}
		 System.out.println("factorial of given numner is =" + count);
		}
	}
		
	}
	

	



